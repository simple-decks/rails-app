<div id="presentation-view">  
<%= markdown(@presentation.content) %>
</div>


<h1>Edit Presentation</h1>
<%= form_for(@presentation, :remote => true, :html => { :class => "update-presentation" }) do |f| %>
<%= f.text_area :content, placeholder:"Add presentation content" %>
<p><%= f.submit %>
</p>
<% end %>

